<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:themes="clr-namespace:AnakinRaW.CommonUtilities.Wpf">


    <SolidColorBrush x:Key="{x:Static themes:FluentColors.TextFillPrimary}" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="{x:Static themes:FluentColors.SubtleFillSecondary}" Color="#FF373737"/>
    <SolidColorBrush x:Key="{x:Static themes:FluentColors.TextFillSecondary}" Color="#FFD1D1D1"/>
    <SolidColorBrush x:Key="{x:Static themes:FluentColors.SubtleFillTertiary}" Color="#FF323232"/>

    <SolidColorBrush x:Key="{x:Static themes:FluentColors.AccentFillDefault}" Color="#FF007ACC"/>

</ResourceDictionary>
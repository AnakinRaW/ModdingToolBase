<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:themes="clr-namespace:AnakinRaW.CommonUtilities.Wpf">


    <SolidColorBrush x:Key="{x:Static themes:FluentColors.TextFillPrimary}" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="{x:Static themes:FluentColors.SubtleFillSecondary}" Color="#FF373737"/>
    <SolidColorBrush x:Key="{x:Static themes:FluentColors.TextFillSecondary}" Color="#FFD1D1D1"/>
    <SolidColorBrush x:Key="{x:Static themes:FluentColors.SubtleFillTertiary}" Color="#FF323232"/>

    <SolidColorBrush x:Key="{x:Static themes:FluentColors.AccentFillDefault}" Color="#FF007ACC"/>

    <SolidColorBrush x:Key="{x:Static themes:FluentColors.SolidBackgroundFillTertiary}" Color="#FF292929"/>
    <SolidColorBrush x:Key="{x:Static themes:FluentColors.SolidBackgroundFillSecondary}" Color="#FF1F1F1F"/>

    <Color x:Key="{x:Static themes:FluentColors.FocusStrokeOuter}" A="FF" B="FF" G="FF" R="FF"/>
    <Color x:Key="{x:Static themes:FluentColors.PopupShadow}" A="42" B="00" G="00" R="00"/>

    <SolidColorBrush x:Key="{x:Static themes:FluentColors.TextFillDisabled}" Color="#FF707070"/>
    <SolidColorBrush x:Key="{x:Static themes:FluentColors.PopupBorder}" Color="#FF1F1F1F"/>

</ResourceDictionary>